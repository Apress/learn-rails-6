<%= render "timelines/profile_header", user: @visited_user %>

<% if on_self? %>
  <%= render("posts/form")  %>
<% end %>

<% if can? current_user, :see_timeline, @visited_user %>
  <%= render "timelines/all" %>
<% else %>
  <p style="text-align: center">
    Post ditampilkan hanya untuk pengikut
  </p>
<% end %>
